{
  "name": "logary",
  "private": false,
  "version": "6.0.0",
  "description": "Unified behavioural analytics; logging, metrics, tracing.",
  "author": "Henrik Feldt",
  "license": "GPL3 or commercial",
  "main": "dist/index",
  "types": "dist/index",
  "exports": {
    ".": "./dist/index.js",
    "./targets/*": "./dist/targets/*"
  },
  "homepage": "https://logary.tech",
  "files": [
    "dist/**/*",
    "package.json"
  ],
  "repository": "git://github.com/logary/logary-js.git",
  "keywords": [
    "exception",
    "error",
    "apm",
    "analytics",
    "logging",
    "logs",
    "funnel"
  ],
  "browserslist": "> 0.5%, not dead",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --ext .ts src",
    "prepublishOnly": "yarn test && yarn run lint",
    "install-peers": "install-peers -f"
  },
  "dependencies": {
    "@opentelemetry/api": "^0.10.2",
    "@opentelemetry/tracing": "^0.10.2",
    "node-abort-controller": "^1.1.0",
    "jssha": "^3.1.0",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^26.0.0",
    "@types/jssha": "^2.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.13",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.1",
    "ts-jest": "^26.0.0",
    "typedoc": "^0.17.0",
    "typescript": "^3.8.3"
  }
}
